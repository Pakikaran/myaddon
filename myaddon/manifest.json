{
  "manifest_version": 2,

  "name": "MY Addon",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "hi.jpeg",
    "default_popup": "popup.html"
	

  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"https://*/*",
	"http://*/*"
  ]
}